{
  "name": "spaaza.component.receipt",
  "version": "1.0.0",
  "description": "Render a Spaaza Receipt",
  "main": "index.js",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --history-api-fallback",
    "build": "node ./node_modules/webpack/bin/webpack.js -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "start": "babel src --out-dir dist",
    "test": "karma start"
  },
  "author": "Natan Santolo",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2017"
    ],
    "plugins": [
      "external-helpers"
    ],
    "ignore": [
      "node_modules/**",
      "**/*.test.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.10.1",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "webcomponents.js": "github:webcomponents/webcomponentsjs"
  }
}
